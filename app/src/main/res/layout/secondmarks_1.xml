<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#C1E3DF"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">


        <LinearLayout
            android:layout_width="130dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="2dp"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tv_Subject21"
                android:layout_width="130dp"
                android:layout_height="30dp"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="5dp"
                android:layout_weight="2"
                android:text="SUBJECTS"
                android:textColor="#000000"
                android:textSize="20sp"
                android:textStyle="bold" />


            <TextView
                android:id="@+id/tv_PS"
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:layout_weight="2"
                android:text="@string/PS"
                android:textColor="#000000"
                android:textSize="20sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/tv_MFCS"
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:layout_weight="2"
                android:text="@string/Mfcs"
                android:textColor="#000000"
                android:textSize="20sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/tv_DS"
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:layout_weight="2"
                android:text="@string/Ds"
                android:textColor="#000000"
                android:textSize="20sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/tv_DLD"
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:layout_weight="2"
                android:text="@string/Dld"
                android:textColor="#000000"
                android:textSize="20sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/tv_EDC"
                android:layout_width="130dp"
                android:layout_height="30dp"
                android:layout_marginTop="15dp"
                android:layout_weight="2"
                android:text="@string/Edc"
                android:textColor="#000000"
                android:textSize="20sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/tv_BEE"
                android:layout_width="130dp"
                android:layout_height="30dp"
                android:layout_marginTop="15dp"
                android:layout_weight="2"
                android:text="@string/Bee"
                android:textColor="#000000"
                android:textSize="20sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/tv_EE_Lab"
                android:layout_width="130dp"
                android:layout_height="30dp"
                android:layout_marginTop="15dp"
                android:layout_weight="2"
                android:text="@string/EE_Lab"
                android:textColor="#000000"
                android:textSize="20sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/tv_DS_Lab"
                android:layout_width="130dp"
                android:layout_height="30dp"
                android:layout_marginBottom="2dp"
                android:layout_marginTop="2dp"
                android:layout_weight="2"
                android:text="@string/Ds_Lab"
                android:textColor="#000000"
                android:textSize="20sp"
                android:textStyle="bold" />

        </LinearLayout>

        //INTERNAL LAYOUT

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tv_Internal21"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="5dp"
                android:text="INTERNAL"
                android:textColor="#000000"
                android:textSize="20sp"
                android:textStyle="bold" />



            <EditText
                android:id="@+id/et_Int_PS"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:background="#FFFFFF"
                android:hint="25"
                android:inputType="numberSigned"
                android:maxLength="2"
                android:textColor="#000000"
                android:textSize="30sp" />

            <EditText
                android:id="@+id/et_Int_MFCS"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:background="#FFFFFF"
                android:hint="25"
                android:inputType="numberSigned"
                android:maxLength="2"
                android:textColor="#000000"
                android:textSize="30sp" />

            <EditText
                android:id="@+id/et_Int_DS"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:background="#FFFFFF"
                android:hint="25"
                android:inputType="numberSigned"
                android:maxLength="2"
                android:textColor="#000000"
                android:textSize="30sp" />

            <EditText
                android:id="@+id/et_Int_DLD"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:background="#FFFFFF"
                android:hint="25"
                android:inputType="numberSigned"
                android:maxLength="2"
                android:textColor="#000000"
                android:textSize="30sp" />

            <EditText
                android:id="@+id/et_Int_EDC"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:background="#FFFFFF"
                android:hint="25"
                android:inputType="numberSigned"
                android:maxLength="2"
                android:textColor="#000000"
                android:textSize="30sp" />

            <EditText
                android:id="@+id/et_Int_BEE"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:background="#FFFFFF"
                android:hint="25"
                android:inputType="numberSigned"
                android:maxLength="2"
                android:textColor="#000000"
                android:textSize="30sp" />


            <EditText
                android:id="@+id/et_Int_EELab"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:background="#FFFFFF"
                android:hint="25"
                android:inputType="numberSigned"
                android:maxLength="2"
                android:textColor="#000000"
                android:textSize="30sp" />

            <EditText
                android:id="@+id/et_Int_DSLab"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:background="#FFFFFF"
                android:hint="25"
                android:inputType="numberSigned"
                android:maxLength="2"
                android:textColor="#000000"
                android:textSize="30sp" />


        </LinearLayout>

        //EXTERNAL LAYOUT

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_weight="1"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tv_External21"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="5dp"
                android:text="EXTERNAL"
                android:textColor="#000000"
                android:textSize="20sp"
                android:textStyle="bold" />



            <EditText
                android:id="@+id/et_Ext_PS"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:background="#FFFFFF"
                android:hint="75"
                android:inputType="numberSigned"
                android:maxLength="2"
                android:textColor="#000000"
                android:textSize="30sp" />

            <EditText
                android:id="@+id/et_Ext_MFCS"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:background="#FFFFFF"
                android:hint="75"
                android:inputType="numberSigned"
                android:maxLength="2"
                android:textColor="#000000"
                android:textSize="30sp" />

            <EditText
                android:id="@+id/et_Ext_DS"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:background="#FFFFFF"
                android:hint="75"
                android:inputType="numberSigned"
                android:maxLength="2"
                android:textColor="#000000"
                android:textSize="30sp" />

            <EditText
                android:id="@+id/et_Ext_DLD"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:background="#FFFFFF"
                android:hint="75"
                android:inputType="numberSigned"
                android:maxLength="2"
                android:textColor="#000000"
                android:textSize="30sp" />

            <EditText
                android:id="@+id/et_Ext_EDC"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:background="#FFFFFF"
                android:hint="75"
                android:inputType="numberSigned"
                android:maxLength="2"
                android:textColor="#000000"
                android:textSize="30sp" />

            <EditText
                android:id="@+id/et_Ext_BEE"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:background="#FFFFFF"
                android:hint="75"
                android:inputType="numberSigned"
                android:maxLength="2"
                android:textColor="#000000"
                android:textSize="30sp" />

            <EditText
                android:id="@+id/et_Ext_EELab"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:background="#FFFFFF"
                android:hint="75"
                android:inputType="numberSigned"
                android:maxLength="2"
                android:textColor="#000000"
                android:textSize="30sp" />

            <EditText
                android:id="@+id/et_Ext_DSLab"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="5dp"
                android:background="#FFFFFF"
                android:hint="75"
                android:inputType="numberSigned"
                android:maxLength="2"
                android:textColor="#000000"
                android:textSize="30sp" />


        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:weightSum="2">

        <Button
            android:id="@+id/btn_Ok21"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="1dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="5dp"
            android:layout_weight="1"
            android:background="@drawable/button_normal"
            android:text="Calculate"
            android:textColor="#000000"
            android:textSize="25sp"
            android:textStyle="bold" />

        <Button
            android:id="@+id/btn_Cal21"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_marginBottom="1dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="5dp"
            android:layout_weight="1"
            android:background="@drawable/button_normal"
            android:text="Result"
            android:textColor="#000000"
            android:textSize="25sp"
            android:textStyle="bold" />


    </LinearLayout>


</LinearLayout>